<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="RemoHeadlessExtensionId"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="hu.bme.mit.remo.scverif.headless.RemoHeadlessApplication">
         </run>
      </application>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="hu.bme.mit.remo.scverif.headless.RemoHeadlessExtensionId"
            name="RemoHeadlessProduct">
         <property
               name="appName"
               value="RemoHeadlessProduct">
         </property>
      </product>
   </extension>

</plugin>
